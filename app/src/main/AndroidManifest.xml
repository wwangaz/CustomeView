<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wangweimin.customerview">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ViewListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name=".Test"
            android:enabled="false"
            android:icon="@mipmap/iconmonstr"
            android:label="更换icon"
            android:targetActivity=".activity.ViewListActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity-alias>


        <activity
            android:name=".activity.ShowActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.FlopActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.ShowViewActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.AutoCompleteActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.PieChartActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.RotateMenuActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity.HackDialogServiceActivity"
            android:screenOrientation="portrait" />

        <activity android:name=".activity.GraduallyViewActivity" />

        <activity android:name=".activity.PathAnimActivity" />

        <activity android:name=".activity.SvgActivity" />

        <activity android:name=".activity.SurfaceViewTestActivity" />

        <activity android:name=".activity.ColorMatrixActivity" />

        <activity android:name=".activity.PullToLoadMoreActivity" />

        <activity android:name=".activity.AnimateChartActivity" />

        <service android:name=".service.HackDialogService" />

        <activity android:name=".activity.CircleLoadingViewActivity" />

        <activity android:name=".activity.ConstraintCircleActivity" />

        <activity android:name=".activity.StickHeadActivity"/>
    </application>

</manifest>
